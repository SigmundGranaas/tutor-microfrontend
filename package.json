{
  "name": "edX-frontend-application",
  "version": "0.1.0",
  "description": "Marketing micro-service for the edX-platform REST API using edX/frontend-template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NTNUBeta/edX-frontend-application.git"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/edX-frontend-application#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/edX-frontend-application/issues"
  },
  "dependencies": {
    "@edx/frontend-platform": "^1.3.7",
    "@edx/paragon": "^7.2.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^6.6.1",
    "history": "^4.1.0",
    "immutable": "^4.0.0-rc.12",
    "bootstrap": "^4.5.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.6.1",
    "history": "^4.1.0",
    "immutable": "^4.0.0-rc.12",
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2",
    "lodash.memoize": "^4.1.2",
    "lodash.pick": "^4.4.0",
    "lodash.snakecase": "^4.1.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.1",
    "react-card-carousel": "^1.1.3",
